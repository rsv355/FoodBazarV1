<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="foodbazar.webmyne.com.foodbazar" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >



        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>

        <activity android:name=".Settings"/>
        <activity android:name=".Profile"/>
        <activity android:name=".MenuListActivity"/>
        <activity android:name=".MenuItemListActivity"/>
        <activity android:name=".MenuItemDetailActivity"/>
        <activity android:name=".CartActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".RateActivity"/>
        <activity android:name=".ContactActivity"/>
        <activity android:name=".AddToCart"/>
        <activity android:name=".WalletActivity"/>
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".HomeScreen"/>


    </application>

</manifest>
